# Application Name
APP_NAME=jacob

# Google Maps API Key (REQUIRED for map display)
GOOGLE_MAPS_API_KEY=YOUR_API_KEY_HERE

# Temporal Configuration (defaults for local development)
TEMPORAL_HOST=127.0.0.1
TEMPORAL_PORT=7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=party-queue

# Temporal Cloud Configuration
# Choose ONE of three authentication methods:

# Option 1: API Key Authentication (Recommended - Simplest)
# TEMPORAL_TLS_ENABLED=true
# TEMPORAL_HOST=us-east-1.aws.api.temporal.io
# TEMPORAL_NAMESPACE=your-namespace
# TEMPORAL_API_KEY=your-api-key-here

# Option 2: mTLS Certificate Authentication (Legacy)
# TEMPORAL_TLS_ENABLED=true
# TEMPORAL_HOST=your-namespace.tmprl.cloud
# TEMPORAL_NAMESPACE=your-namespace
# TEMPORAL_CLIENT_CERT=certs/client.pem
# TEMPORAL_CLIENT_KEY=certs/client.key

# Option 3: Both (Maximum Security - mTLS + API Key)
# TEMPORAL_TLS_ENABLED=true
# TEMPORAL_HOST=your-namespace.tmprl.cloud
# TEMPORAL_NAMESPACE=your-namespace
# TEMPORAL_API_KEY=your-api-key-here
# TEMPORAL_CLIENT_CERT=certs/client.pem
# TEMPORAL_CLIENT_KEY=certs/client.key

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080

# Device Authentication (leave empty to allow all devices)
# Comma-separated UUIDs: DEVICE_WHITELIST=uuid1,uuid2,uuid3
DEVICE_WHITELIST=
