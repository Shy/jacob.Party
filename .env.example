# Application Name
# This name will appear in the web interface and be used throughout the app
APP_NAME=jacob

# Google Maps API Key
# Get your key at: https://developers.google.com/maps/documentation/javascript/get-api-key
GOOGLE_MAPS_API_KEY=YOUR_API_KEY_HERE

# Temporal Configuration
# For local development (temporal server start-dev):
TEMPORAL_HOST=127.0.0.1
TEMPORAL_PORT=7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=party-queue

# For Temporal Cloud (uncomment and configure):
# TEMPORAL_HOST=your-namespace.tmprl.cloud
# TEMPORAL_NAMESPACE=your-namespace
# TEMPORAL_TASK_QUEUE=party-queue
# TEMPORAL_TLS_ENABLED=true
# TEMPORAL_CLIENT_CERT=certs/client.pem
# TEMPORAL_CLIENT_KEY=certs/client.key

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080

# Device Authentication
# Comma-separated list of allowed device UUIDs
# Leave empty to allow all devices (development mode)
ALLOWED_DEVICE_IDS=
